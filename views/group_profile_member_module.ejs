<div class="timeline module">
	<div class="timeline-header">
    	<b>Members</b>
    </div>
    <ul>
	<% for(var i=members.length-1; i>=0; i--) {%>
	  	<li>
	  		<div>
		   		<a href="/<%= members[i].username %>">
		   			<div class="separator owner">
		   				<%= members[i].fullname %><small> @<%= members[i].username %></small>
		   			</div>
		   		</a>
		   	</div>
	   	</li>
	<% } %>
	</ul>
</div>